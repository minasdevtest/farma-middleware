{
  "name": "farmacia-middleware",
  "description": "Midleware do projeto Farmácia Solidária",
  "version": "1.0.0",
  "engines": {
    "node": "11.10.0"
  },
  "main": "dist",
  "scripts": {
    "heroku-postbuild": "npm run build",
    "dev": "PORT=3010 nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "dev-docker": "docker-compose up middleware",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "build-docker": "docker build .",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "developit/express-es6-rest-api",
  "author": "Jason Miller <jason@developit.ca>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-http-proxy": "^1.5.1",
    "http-proxy": "^1.17.0",
    "http-proxy-middleware": "^0.19.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.2.2",
    "mongoose": "^5.4.20",
    "morgan": "^1.9.1",
    "node-fetch": "^2.3.0",
    "resource-router-middleware": "^0.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^5.14.1",
    "nodemon": "^1.18.10"
  }
}
